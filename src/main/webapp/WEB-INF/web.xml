<web-app>
	<display-name>OurFi Server</display-name>
	<servlet>
		<servlet-name>JSONDispatcherServlet</servlet-name>
		<display-name>JSONDispatcherServlet</display-name>
		<description></description>
		<servlet-class>br.com.ourfi.server.JSONDispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSONDispatcherServlet</servlet-name>
		<url-pattern>/services/ourfi/*</url-pattern>
	</servlet-mapping>
</web-app>
